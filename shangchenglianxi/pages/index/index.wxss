.index_swiper{

}
/* 轮播图里面的样式 */
swiper{
    width: 750rpx;
    height: 340rpx;
}
.index_swiper swiper image{
    width: 100%;
}

/* 导航栏的样式 */
.index_cate{
display: flex;
}
.index_cate navigator{
    flex: 1;
    padding: 10rpx;
}
.index_cate navigator image{
width: 100%;
}
/* 楼层样式 */
.index_floor{

}
.index_floor .florr_group{

} 
.index_floor .florr_group .floor_title{

}
.index_floor .florr_group .floor_title image{
    padding: 10rpx 0;
    width: 100%;
}
.index_floor .florr_group .floor_list{
    /* 清除浮动 */
overflow: hidden;
}
.index_floor .florr_group .floor_list navigator{
    float: left;
    width: 33.33%;
}
/* 后四个超链接 */
/* 原图的宽高是232 * 286 */

.index_floor .florr_group .floor_list navigator:nth-last-child(-n+4){
    height: calc(38.33vw * 386 / 232 / 2);
    border-left:10rpx solid #fff;
}
/* 给2 3 两个超链接 */
.index_floor .florr_group .floor_list navigator:nth-child(2),.index_floor .florr_group .floor_list navigator:nth-child(3){
border-bottom: 10rpx solid #fff;
}
.index_floor .florr_group .floor_list navigator:nth-last-child(-n+4) image{
    width: 100%;
    height: 100%;
}
.index_floor .florr_group .floor_list navigator image{
    width: 100%;
}
